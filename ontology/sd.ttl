###  https://bimerr.iot.linkeddata.es/def/sensor-data#Actuator
sd:Actuator rdf:type owl:Class ;
            rdfs:subClassOf sd:Device ;
            rdfs:label "Actuator"@en .


###  https://bimerr.iot.linkeddata.es/def/sensor-data#Device
sd:Device rdf:type owl:Class ;
          rdfs:subClassOf saref:Device ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sd:baseName ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sd:baseSum ;
                            owl:someValuesFrom xsd:float
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sd:baseTime ;
                            owl:someValuesFrom xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sd:baseValue ;
                            owl:someValuesFrom xsd:float
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sd:baseVersion ;
                            owl:someValuesFrom xsd:integer
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sd:name ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sd:sum ;
                            owl:someValuesFrom xsd:float
                          ] ;
          rdfs:label "SenML device"@en .


###  https://bimerr.iot.linkeddata.es/def/sensor-data#Sensor
sd:Sensor rdf:type owl:Class ;
          rdfs:subClassOf sd:Device ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sd:updateTime ;
                            owl:someValuesFrom xsd:integer
                          ] ;
          rdfs:label "Sensor"@en .


###  https://w3id.org/saref#Device
saref:Device rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty saref:makesMeasurement ;
                               owl:someValuesFrom saref:Measurement
                             ] ;
             rdfs:label "Device"@en .


###  https://w3id.org/saref#FeatureOfInterest
saref:FeatureOfInterest rdf:type owl:Class ;
                        rdfs:label "Feature of interest"@en .


###  https://w3id.org/saref#Measurement
saref:Measurement rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty saref:isMeasuredIn ;
                                    owl:someValuesFrom saref:UnitOfMeasure
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty saref:isMeasurementOf ;
                                    owl:someValuesFrom saref:FeatureOfInterest
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty saref:relatesToProperty ;
                                    owl:someValuesFrom saref:Property
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty saref:hasTimeStamp ;
                                    owl:someValuesFrom xsd:dateTimeStamp
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty saref:hasValue ;
                                    owl:someValuesFrom xsd:float
                                  ] ;
                  rdfs:label "Measurement"@en .


###  https://w3id.org/saref#Property
saref:Property rdf:type owl:Class ;
               rdfs:label "Property"@en .


###  https://w3id.org/saref#UnitOfMeasure
saref:UnitOfMeasure rdf:type owl:Class ;
