###  https://bimerr.iot.linkeddata.es/def/sensor-data#Actuator
:Actuator rdf:type owl:Class ;
          rdfs:subClassOf :Device ;
          rdfs:label "Actuator"@en .


###  https://bimerr.iot.linkeddata.es/def/sensor-data#Device
:Device rdf:type owl:Class ;
        rdfs:subClassOf <https://w3id.org/saref#Device> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :baseName ;
                          owl:someValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :baseSum ;
                          owl:someValuesFrom xsd:float
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :baseTime ;
                          owl:someValuesFrom xsd:dateTime
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :baseValue ;
                          owl:someValuesFrom xsd:float
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :baseVersion ;
                          owl:someValuesFrom xsd:integer
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :name ;
                          owl:someValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :sum ;
                          owl:someValuesFrom xsd:float
                        ] ;
        rdfs:label "SenML device"@en .


###  https://bimerr.iot.linkeddata.es/def/sensor-data#Sensor
:Sensor rdf:type owl:Class ;
        rdfs:subClassOf :Device ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :updateTime ;
                          owl:someValuesFrom xsd:integer
                        ] ;
        rdfs:label "Sensor"@en .


